{
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"env": {
		"node": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"],
	"settings": {
		"react": {
			"version": "16.8"
		}
	},
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"react/prop-types": "off",
		"no-console": "off"
	}
}
